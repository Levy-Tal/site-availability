    "panels": [
      {
        "type": "timeseries",
        "title": "Goroutines",
        "id": 1,
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "targets": [
          {
            "expr": "go_goroutines",
            "refId": "A"
          }
        ],
        "gridPos": { "x": 0, "y": 0, "w": 6, "h": 6 },
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "palette-classic" },
            "custom": {
              "drawStyle": "line",
              "lineWidth": 1,
              "showPoints": "auto"
            }
          },
          "overrides": []
        }
      },
      {
        "type": "timeseries",
        "title": "Heap Allocation",
        "id": 2,
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "targets": [
          {
            "expr": "go_memstats_alloc_bytes",
            "refId": "B"
          }
        ],
        "gridPos": { "x": 6, "y": 0, "w": 6, "h": 6 },
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "color": { "mode": "palette-classic" },
            "custom": { "drawStyle": "line", "lineWidth": 1 }
          },
          "overrides": []
        }
      },
